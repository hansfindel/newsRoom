<h1>NewsRoom</h1>
<h2> Noticias principales </h2>
<div class="row">
  <div class="span8 offset2">
    <% @articles.each do |article| %>
    <div class="well">
      <% cache article do %>
      	<%= render article %>
      <% end %>
    </div>
    <% end %>
    <%= paginate @articles, :window => 2 %>
  </div>
</div>


<% if can? :create, Article %>
<%= link_to 'New Article', new_article_path %>
<%= link_to "Populate by glutton", store_glutton_feeds_path %>
<% end %>