<h1>Listing news_agencies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news_agencies.each do |news_agency| %>
  <tr>
    <td><%= news_agency.name %></td>
    <td><%= news_agency.url %></td>
    <td><%= link_to 'Show', news_agency %></td>
    <td><%= link_to 'Edit', edit_news_agency_path(news_agency) %></td>
    <td><%= link_to 'Destroy', news_agency, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News agency', new_news_agency_path %>
